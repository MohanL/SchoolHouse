<%= month_calendar events: @student_classes do |date| %>
  <%= date.day %>
  <% @student_classes.each do |student_class| %>
    <div>
      <%= student_class.name if student_class.meets_on.any?{|day| /#{date.strftime('%A')}/.match(day) } %>
    </div>
  <% end %>
<% end %>

<h1>Showing all Parents</h1>
<ul>
<% @users.each do |user| %>
  <li><%= link_to user.name, user %> (
    <% user.students.each do |student| %>
      <%= link_to student.name, student  %> 
    <% end %>
    )</li>
<% end %>
</ul>
